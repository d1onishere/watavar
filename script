firetouchinterest(torso, balls2, 1)
firetouchinterest(left, balls2, 1)
firetouchinterest(leg, balls2, 1)
firetouchinterest(head, balls2, 1)
               end
            end
        end
        end
end
end)


setsimulationradius(math.huge, math.huge)
