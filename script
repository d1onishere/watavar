local Tab = Window:NewTab("WFL")

local Section = Tab:NewSection("Firetouch")
Section:NewButton("Enable", "ButtonInfo", function()
    print("Clicked")

    _G.BallName = "l̸̼̔il̷͎̅ḭ̴͘iḮ̷̙il̶̼̈́il̴̘̕Ĩ̵̹į̴̌"
