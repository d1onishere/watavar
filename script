local Section = Tab:NewSection("Firetouch Slider")
Section:NewSlider("Slider", "pro", 100, 0, function(s) -- 100 (MaxValue) | 0 (MinValue)
    _G.BallName = "TPS"
_G.Magnitude = s
_G.Enabled = true


-- DONT TOUCH ANYTHING BELOW THIS

_G.Path = nil

